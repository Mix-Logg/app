{
  "expo": {
    "name": "MIX",
    "slug": "MIX",
    "version": "3.0.0",
    "runtimeVersion": "3.0.0",
    "orientation": "portrait",
    "icon": "./img/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "A MIX precisa utilizar seu localização."
        }
      ]
    ],
    "splash": {
      "image": "./img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./img/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mixtrans.MIX",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDwhBCpqKMzkEpXm8w-t3Ib0KDOM9vdUPs"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./img/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "954638a6-a9ee-44c7-8b16-e0a2f88c3eb7"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/954638a6-a9ee-44c7-8b16-e0a2f88c3eb7"
    }
  }
}

{
  "expo": {
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow MIX to use your location."
        }
      ]
    ],
    "name": "MIX",
    "slug": "MIX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./img/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./img/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mixtrans.MIX"
    },
    "web": {
      "favicon": "./img/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "954638a6-a9ee-44c7-8b16-e0a2f88c3eb7"
      }
    }
  }
}

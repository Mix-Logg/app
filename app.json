{
  "expo": {
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "name": "MIX",
    "slug": "MIX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/main/res/drawable-mdpi/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/main/res/drawable-mdpi/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/main/res/drawable-mdpi/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mixtrans.MIX"
    },
    "web": {
      "favicon": "./src/main/res/drawable-mdpi/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "954638a6-a9ee-44c7-8b16-e0a2f88c3eb7"
      }
    }
  }
}
